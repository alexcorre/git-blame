{
  "extends": "@workpop/eslint-config-workpop",
  "globals": {
    "atom": false
  },
  "settings": {
    "import/core-modules": [
      "atom"
    ]
  },
  "rules": {
    "import/prefer-default-export": 0,
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "react/prop-types": 0
  }
}
